{% if sponsors %}
    <h2>Спонсоры</h2>
    {% for category, csponsors in sponsors %}
         <h4>{{ category }} </h4>
         {% for sponsor in csponsors %}
            <div class="partner sort-order-{{ loop.index }}">
                <a href="{{ sponsor.site }}">
                    <img src="{{ asset(sponsor.logo) }}" alt="{{ sponsor.name }}" title="{{ sponsor.name }}" />
                </a>
            </div>
         {% endfor %}
    {% endfor %}
{% endif %}

<div class="become-partner">
    <a href="{{ path('page_show', {'slug': 'partners'}) }}#become-partner">
        Здесь может<br> быть ваш логотип
    </a>
</div>
