{% extends 'StfalconEventBundle::layout.html.twig' %}

{% block title %}Докладчики / {{ parent() }}{% endblock %}

{% block event_speakers_widget %}{% endblock %}

{% block content %}
    <!-- Контент участников -->
    <h2>Участники</h2>

    <ul class="participants">
        {% include "StfalconEventBundle:Participant:list_participants.html.twig" with { "participants": participants } %}
    </ul>
    <div class="preloader">
        <img src="{{ asset('/images/preloader.gif') }}" alt="" />
    </div>
    <!-- /Контент участников -->
{% endblock %}
{% block javascripts %}
    {% javascripts 'js/jquery-1.8.2.min.js' 'js/scroll-load.js' %}
    <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}
