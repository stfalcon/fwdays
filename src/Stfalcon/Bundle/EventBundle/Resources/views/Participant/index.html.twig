{% extends 'StfalconEventBundle::layout.html.twig' %}

{% block title %}Докладчики / {{ parent() }}{% endblock %}

{% block event_speakers_widget %}{% endblock %}

{% block content %}
    <!-- Контент участников -->
    <h2>Участники</h2>

    <ul class="participants">
        {% block ajax_content %}{% endblock %}
    </ul>
    <div class="preloader">
        <img src="{{ asset('/images/preloader.gif') }}" alt="" />
    </div>
    <!-- /Контент участников -->
{% endblock %}

{% block javascripts %}
    {% javascripts 'js/jquery-1.8.2.min.js' 'bundles/stfalconevent/js/participants-scroll.js' %}
    <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}
